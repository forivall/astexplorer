{
  "name": "tacoscript-ast-playground",
  "version": "0.1.0",
  "description": "Online demo of tacoscript, horchata and babylon",
  "repository": {
    "type": "git",
    "url": "https://github.com/fkling/esprima_ast_explorer.git"
  },
  "author": "Felix Kling",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^5.8.3",
    "babelify": "^6.1.3",
    "brfs": "^1.4.1",
    "browserify": "^11.0.0",
    "classnames": "^2.1.3",
    "escodegen": "^1.6.1",
    "http-server": "^0.8.0",
    "partition-bundle": "^2.0.0",
    "reactify": "^1.0.0",
    "shelljs": "^0.5.1",
    "uglify-js": "^2.4.16",
    "uglifyify": "^3.0.1",
    "watchify": "^3.3.1"
  },
  "//dependencies": {
    "horchata": "*"
  },
  "dependencies": {
    "babel-core": "^5.7.3",
    "babel-runtime": "^5.8.19",
    "babylon": "^5.8.22",
    "classnames": "^2.1.3",
    "codemirror": "^5.1.0",
    "everything.js": "^1.0.3",
    "espree": "^2.2.4",
    "jscodeshift": "^0.3",
    "keypress": "git://github.com/dmauro/Keypress",
    "pubsub-js": "^1.4.2",
    "react": "^0.13.1",
    "recast": "^0.10.32",
    "tacoscript-core": "*"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": 0,
          "optional": [
            "runtime"
          ]
        }
      ],
      "brfs"
    ]
  },
  "scripts": {
    "start": "watchify -v --iw 'node_modules/(!tacoscript-core)/**' --debug src/app.js -o dist/app.js & http-server",
    "build-production": "NODE_ENV=production browserify src/app.js -o dist/app.js -g [uglifyify -b beautify=false,ascii-only=true]",
    "build": "browserify src/app.js -o dist/app.js"
  }
}
