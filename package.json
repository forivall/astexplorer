    "envify": "^3.2.0",
    "escodegen": "^1.6.1",
    "http-server": "^0.8.0",
    "partition-bundle": "^1.3.0",
    "reactify": "^1.0.0",
    "uglify-js": "^2.4.16",
    "uglifyify": "^3.0.1",
    "watchify": "^3.2.3"
  },
  "//dependencies": {
    "horchata": "*"
  },
